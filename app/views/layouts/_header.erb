<%= form_with local: true do |form| %>
<div id="top_menu">
  <ul>
    <li>
      <%= form.label :user %>
      <%= form.text_field :user %>
    </li>
    <li>
      <%= form.label :password %>
      <%= form.password_field :password %>
    </li>
    <li>
      <%= form.label :remember_me %>
      <%= form.check_box :remember_me %>
    </li>
    <li>
      <%= form.submit :Identificarme %>
    </li>
    |
    <li>
      <%= link_to 'Register' %>
    </li>
  </ul>
</div>
<% end %>